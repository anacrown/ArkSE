<?xml version="1.0" encoding="utf-8" ?>
<pages:BasePage x:TypeArguments="officialservers:OfficialServersViewModel" 
                xmlns="http://xamarin.com/schemas/2014/forms"
                xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                xmlns:pages="clr-namespace:ArkSE.UI.Pages;assembly=ArkSE"
                xmlns:officialservers="clr-namespace:ArkSE.BL.ViewModels.OfficialServers"
                xmlns:dataObjects="clr-namespace:ArkSE.DAL.DataObjects;assembly=ArkSE.DAL"
                xmlns:officialServers="clr-namespace:ArkSE.UI.Pages.OfficialServers;assembly=ArkSE"
                x:DataType="officialservers:OfficialServersViewModel"
                x:Class="ArkSE.UI.Pages.OfficialServers.OfficialServersPage">
    <ContentPage.Content>
        <ListView ItemsSource="{Binding OfficialServers}"
                  ItemTemplate="{StaticResource OfficialServerObjectViewModelDataTemplate}"
                  SelectionMode="None" ItemTapped="ListView_OnItemTapped">
        </ListView>
    </ContentPage.Content>
</pages:BasePage>