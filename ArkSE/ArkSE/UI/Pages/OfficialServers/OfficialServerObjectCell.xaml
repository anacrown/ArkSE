<?xml version="1.0" encoding="UTF-8"?>
<pages:BaseElement xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dataObjects="clr-namespace:ArkSE.DAL.DataObjects;assembly=ArkSE.DAL"
             xmlns:pages="clr-namespace:ArkSE.UI.Pages;assembly=ArkSE"
             xmlns:serverInfo="clr-namespace:ArkSE.BL.ViewModels.ServerInfo;assembly=ArkSE"
             xmlns:officialServers="clr-namespace:ArkSE.BL.ViewModels.OfficialServers;assembly=ArkSE"
             x:Class="ArkSE.UI.Pages.OfficialServers.OfficialServerObjectCell" 
             x:DataType="officialServers:OfficialServerObjectViewModel">
  <ViewCell.View>
      <StackLayout Orientation="Horizontal">
            <Label Text="{Binding ServerObject.Ip}"/>
          <Label Text="["/>
            <Label Text="{Binding ServerObject.Name}"/>
          <Label Text="]"/>
      </StackLayout>
    </ViewCell.View>
</pages:BaseElement>